[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"üöÄ Starting server for testing...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev &"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "SERVER_PID=$!"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"‚è≥ Waiting for server to be ready...\""

[[workflows.workflow.tasks]]
