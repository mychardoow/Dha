[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx tsx production-function-test.ts"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "TEST_EXIT=$?"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"🛑 Stopping test server...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "kill $SERVER_PID 2>/dev/null || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "if [ $TEST_EXIT -eq 0 ]; then"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "  echo \"✅ ALL TESTS PASSED - READY FOR DEPLOYMENT!\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "else"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "  echo \"⚠️ SOME TESTS FAILED - REVIEW REPORT\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "fi"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "exit $TEST_EXIT"

[[workflows.workflow]]
name = "Production Deploy"
author = 48817273
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"🚀 Building for Production...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm install --legacy-peer-deps --no-optional"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"📦 Building client...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd client && npm install --legacy-peer-deps && npm run build && cd .."

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"🔨 Building server...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx tsc --project tsconfig.json --noEmitOnError false || echo \"Build completed with warnings\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"✅ Starting production server...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "NODE_ENV=production PORT=5000 npm start"

[[workflows.workflow]]
name = "Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

