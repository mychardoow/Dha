task = "shell.exec"
args = "exit $TEST_EXIT"

[[workflows.workflow]]
name = "Production Deploy"
author = 48817273
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"üöÄ Building for Production...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm install --legacy-peer-deps --no-optional"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"üì¶ Building client...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd client && npm install --legacy-peer-deps && npm run build && cd .."

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"üî® Building server...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx tsc --project tsconfig.json --noEmitOnError false || echo \"Build completed with warnings\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"‚úÖ Starting production server...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "NODE_ENV=production PORT=5000 npm start"
