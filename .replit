[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"üîç Running comprehensive function tests...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx tsx production-function-test.ts"

[[workflows.workflow.tasks]]
task = "shell.exec"
