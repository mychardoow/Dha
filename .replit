modules = ["nodejs-20", "web", "bash"]
run = "npm start"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-25_05"
packages = ["libuuid", "cairo", "pango", "giflib", "jq", "python3", "gcc", "pkgconf", "pixman", "railway", "gnome-secrets"]

[deployment]
deploymentTarget = "cloudrun"
run = ["sh", "-c", "npm install --legacy-peer-deps && npm run build && npm start"]
build = ["sh", "-c", "bash render-build-production.sh"]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 41631
externalPort = 8008

[[ports]]
localPort = 42489
externalPort = 8080

[[ports]]
